{
    "name": "yet-another-better-oh-bot",
    "version": "4.4.1",
    "description": "Yet another better discord bot for managing office hours",
    "type": "module",
    "scripts": {
        "lint": "eslint src/",
        "build": "echo 'Compiling...' && npx tsc && echo 'Successfully Compiled.'",
        "test": "npx tsc && NODE_ENV=development node --test dist/tests",
        "format": "npx prettier -w .",
        "dev": "npm run build && NODE_ENV=development node --no-warnings=ExperimentalWarning dist/src/app.js",
        "dev_precompiled": "NODE_ENV=development node dist/app.js",
        "prod": "npm run build && NODE_ENV=production pm2 start dist/app.js --name YABOB_PROD",
        "prod_precompiled": "NODE_ENV=production pm2 start dist/app.js --name YABOB_PROD",
        "makeDoc": "npx typedoc --plugin typedoc-plugin-merge-modules ./src --entryPointStrategy expand --mergeModulesMergeMode module --excludeInternal --name \"Yet Another Better Office-hour Bot\" --includeVersion"
    },
    "devDependencies": {
        "@types/node-fetch": "^2.6.11",
        "@typescript-eslint/eslint-plugin": "^7.1.0",
        "@typescript-eslint/parser": "^7.1.0",
        "eslint": "^8.57.0",
        "pino-pretty": "^10.3.1",
        "prettier": "^3.2.5",
        "ts-mockito": "^2.6.1",
        "typedoc": "^0.25.9",
        "typedoc-plugin-merge-modules": "^5.1.0",
        "typescript": "^5.3.3"
    },
    "dependencies": {
        "ascii-table3": "^0.9.0",
        "axios": "^1.6.7",
        "csv-string": "^4.1.1",
        "discord-api-types": "^0.37.71",
        "discord.js": "^14.14.1",
        "firebase-admin": "^12.0.0",
        "google-spreadsheet": "^4.1.1",
        "googleapis": "^133.0.0",
        "json-2-csv": "^5.4.0",
        "lru-cache": "^10.2.0",
        "lz-string": "^1.5.0",
        "pino": "^8.19.0",
        "zod": "^3.22.4"
    },
    "engines": {
        "node": "20.11.1"
    }
}
